class Solution {
    public int minEatingSpeed(int[] piles, int h) {
        int start= 1;
        int end = Integer.MAX_VALUE;
       
        while(start<end){
            int k = start+(end-start)/2;
            int time =0;
            for(int i=0;i<piles.length;i++){
                if(piles[i] <=k) time++;
                else time +=(piles[i]+k-1)/k;
            }
            if(time>h)start=k+1;
            else end=k;

        }
    
        return start;
    }
}
